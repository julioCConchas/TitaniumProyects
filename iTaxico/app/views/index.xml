		<View id="Bar">
			<ImageView id="menuIcon" onClick="showMenu"/>
			<ImageView id="logoBar"/>
		</View>
			<View id="login">
					<ImageView id="logo"/>
					<!--<ImageView id="googleLogin" onClick="login"/>-->
